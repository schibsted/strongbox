{
  "package": {
    "name": "strongbox-cli",
    "repo": "strongbox-rpm",
    "subject": "strongbox",
    "desc": "Secrets management CLI for AWS",
    "website_url": "https://github.com/schibsted/strongbox",
    "issue_tracker_url": "https://github.com/schibsted/strongbox/issues",
    "vcs_url": "https://github.com/schibsted/strongbox.git",
    "licenses": [
      "MIT"
    ],
    "labels": [
      "aws",
      "secrets management"
    ]
  },
  "version": {
    "name": "@PKG_VERSION@",
    "desc": "Automated RPM package release from master",
    "released": "@DATE@"
  },
  "files": [
    {
      "includePattern": "build/distributions/(.*\\.rpm)",
      "uploadPattern": "$1",
      "matrixParams": {
        "override": 1
      }
    }
  ],
  "publish": true
}
